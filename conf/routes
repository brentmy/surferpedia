# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET     /page1                      controllers.Application.page1()
GET     /carissa                    controllers.Application.carissa()
GET     /jake                       controllers.Application.jake()
GET     /mick                       controllers.Application.mick()
GET     /kanoa                      controllers.Application.kanoa()
GET     /Megan                      controllers.Application.Megan()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
